{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only --require dotenv/config src/index.ts",
    "restart": "npm run kill && sleep 2 && npm run dev",
    "kill": "lsof -ti:8080 | xargs kill -9 2>/dev/null || echo 'No process found on port 8080'",
    "build": "tsc",
    "test": "jest",
    "test:unit": "cross-env NODE_ENV=test jest __tests__/unit",
    "test:integration": "cross-env NODE_ENV=test jest __tests__/integration",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "db:reset": "prisma migrate reset --force && prisma migrate deploy",
    "db:seed": "prisma db seed",
    "db:setup": "npm run db:reset && npm run db:seed"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@langchain/community": "^0.3.45",
    "@langchain/core": "^0.3.57",
    "@langchain/openai": "^0.5.11",
    "@prisma/client": "^5.10.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/ms": "^2.1.0",
    "@types/node": "^20.11.19",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "axios": "^1.9.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.4",
    "express": "^4.18.2",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "langchain": "^0.3.27",
    "morgan": "^1.10.0",
    "ms": "^2.1.3",
    "openai": "^4.29.0",
    "prisma": "^5.10.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/helmet": "^0.0.48",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.9",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node-dev": "^2.0.0"
  }
}
